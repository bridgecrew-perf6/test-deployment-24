{
  "name": "test-deployment",
  "version": "1.0.0",
  "description": "",
  "main": ".babelrc.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "cron": "node ./dist/cron.js",
    "queue": "node ./dist/queue.js",
    "transpile": "babel src --out-dir dist --copy-files",
    "clean": "rimraf dist",
    "migrate": "sequelize-cli db:migrate",
    "rollback": "npx sequelize-cli db:migrate:undo:all",
    "undo": "npx sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all --debug",
    "seed-rollback": "sequelize-cli db:seed:undo:all",
    "seed-dummy": "sequelize-cli --seeders-path seeders/dummy/ db:seed:all --debug",
    "seed-demo": "sequelize-cli --seeders-path seeders/demo/ db:seed:all --debug",
    "seed-dummy-rollback": "sequelize-cli --seeders-path seeders/dummy/ db:seed:undo:all --debug",
    "seed-demo-rollback": "sequelize-cli --seeders-path seeders/demo/ db:seed:undo:all --debug",
    "init": "npm run migrate && npm run seed",
    "build": "npm run clean && npm run transpile",
    "dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" ./src/index.js",
    "cron-dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" ./src/cron.js",
    "queue-dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" ./src/queue.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lintfix": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zohaib-cygnis/test-deployment.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.30.0",
    "@slack/webhook": "^6.0.0",
    "aws-sdk": "^2.890.0",
    "axios": "^0.21.1",
    "b64url": "^1.0.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bull": "^3.20.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.4",
    "dollars-to-cents": "^1.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "hasha": "^5.2.2",
    "html-metadata-parser": "^2.0.2",
    "image-type": "^4.1.0",
    "jsonexport": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lumie": "^0.1.2",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.15",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "read-chunk": "^3.2.0",
    "redis": "^3.0.2",
    "sequelize": "^6.6.5",
    "sequelize-noupdate-attributes": "^1.0.0",
    "sharp": "^0.29.1",
    "stripe": "^8.183.0",
    "temp-dir": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "bugs": {
